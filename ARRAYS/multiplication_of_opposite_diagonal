#include <iostream>
using namespace std;

int main() 
{
    int arr[3][3];
    int dia=1;
     for(int i=0;i<3;i++)
    {
      for(int j=0;j<3;j++)
      {
        cin>>arr[i][j];
      }
    }
    
    for(int i=0;i<3;i++)
    {
      for(int j=0;j<3;j++)
      {
        if(i+j==3-1)
        {
           dia=arr[i][j]*dia;
        }
      }
    }
    cout<<dia<<endl;
}
